#set( $dollar = '$' )
Bundle-Version: ${dollar}{project.version}
Bundle-SymbolicName: $(replace;${dollar}{project.artifactId};-;_)

<!--
  by default import + export all packages from the wrapped jarfile
  except the root package "." - this cannot be shared, as it could
  potentially lead to a split-package situation
-->
-classpath: target/classes
Export-Package: *;version="${dollar}{jar.version}"
Import-Package: !., *

<!--
  load project specific settings
-->
-include: details.bnd
